{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "env": { "node": true, "browser": true },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "import/no-unresolved": [2, { "ignore": ["^(all|part):"] }]
  }
}
